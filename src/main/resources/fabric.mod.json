{
  "schemaVersion": 1,
  "name": "Meteor Client",
  "id": "meteor-client",
  "version": "0.6.9",
  "description": "Minecraft utility mod.",
  "authors": [
    "MineGame159",
    "squidoodly",
    "seasnail",
    "(Barely) GreemDev"
  ],
  "contact": {
    "homepage": "https://meteorclient.com",
    "issues": "https://github.com/GreemDev/meteor/issues",
    "sources": "https://github.com/GreemDev/meteor"
  },
  "license": "GPL-3.0",
  "icon": "assets/meteor-client/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "meteordevelopment.meteorclient.MeteorClient"
    ]
  },
  "mixins": [
    "meteor-client.mixins.json",
    "meteor-client-baritone.mixins.json",
    "meteor-client-indigo.mixins.json",
    "meteor-client-sodium.mixins.json",
    "meteor-client-canvas.mixins.json"
  ],
  "accessWidener": "meteor-client.accesswidener",
  "custom": {
    "meteor-client:devbuild": "${devbuild}",
    "meteor-client:color": "145,61,226",
    "meteor-client:commit": "${commit}",
    "meteor-client:kotlin": "${ktlangver}",
    "modmenu": {
      "links": {
        "modmenu.discord": "https://meteorclient.com/"
      }
    }
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=1.19",
    "fabricloader": ">=0.14.0",
    "fabric-language-kotlin": ">=${ktlangver}"
  },
  "breaks": {
    "optifabric": "*",
    "feather": "*",
    "origins": "*",
    "wurst": "*"
  }
}
